<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-4r [%t] %-5p %c - %m%n</pattern>
        </encoder>
    </appender>
    <logger name="com.emc.automation" level="WARN" />
    <root level="WARN">
        <appender-ref ref="CONSOLE" />
    </root>
</configuration>
